	option enabled '1'
	option mode 'restart_iface'
	option period '10m'
	option pinghosts '172.16.0.7'
	option addressfamily 'ipv4'
	option pingperiod '30s'
	option pingsize 'standard'
	option interface 'WG_HOME'
	option mmifacename 'WG_HOME'

