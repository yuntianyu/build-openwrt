
config watchcat
	option mode 'restart_iface'
	option period '10m'
	option pinghosts '192.168.2.17'
	option addressfamily 'ipv4'
	option pingperiod '30s'
	option pingsize 'standard'
	option enabled '1'
	option interface 'WG_DMKJ'
	option mmifacename 'WG_DMKJ'

