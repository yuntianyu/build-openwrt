
config rule 'WDX'
	option dest_ip '0.0.0.0/0'
	option proto 'all'
	option sticky '0'
	option src_ip '192.168.100.0/24'
	option use_policy 'DX'
	option logging '1'

config rule 'FK'
	option src_ip '192.168.220.0/24'
	option dest_ip '0.0.0.0/0'
	option proto 'all'
	option sticky '0'
	option use_policy 'LT'
	option logging '1'

config rule 'WLT'
	option src_ip '192.168.200.0/24'
	option dest_ip '0.0.0.0/0'
	option proto 'all'
	option sticky '0'
	option use_policy 'LT'
	option logging '1'

config globals 'globals'
	option mmx_mask '0x3F00'
	option rtmon_interval '5'
	option logging '1'
	option loglevel 'notice'

config policy 'DX'
	list use_member 'vwan1_m1_w1'
	option last_resort 'unreachable'

config policy 'LT'
	option last_resort 'unreachable'
	list use_member 'vwan1_m1_w2'

config interface 'vwan1'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	list track_ip '223.5.5.5'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'

config interface 'vwan2'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	list track_ip '223.5.5.5'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'

config member 'vwan1_m1_w1'
	option interface 'vwan1'
	option metric '1'
	option weight '1'

config member 'vwan1_m1_w2'
	option interface 'vwan2'
	option metric '1'
	option weight '1'

